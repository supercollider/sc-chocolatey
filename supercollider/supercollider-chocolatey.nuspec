<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>SuperCollider</id>
    <version>3.12.1.0</version>
    <packageSourceUrl>https://github.com/supercollider/sc-chocolatey</packageSourceUrl>
    <title>SuperCollider for Windows</title>
    <owners>Mike Hodnick</owners>
    <authors>SuperCollider Authors</authors>
    <projectUrl>https://supercollider.github.io</projectUrl>
    <iconUrl>https://cdn.statically.io/gh/supercollider/supercollider.github.io/f443aa3dc03df0d17c0742af646752b215cd8ff7/images/favicon.png</iconUrl>
    <licenseUrl>http://www.gnu.org/licenses/gpl.html</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectSourceUrl>https://github.com/supercollider/supercollider</projectSourceUrl>
    <docsUrl>http://doc.sccode.org/</docsUrl>
    <mailingListUrl>http://new-supercollider-mailing-lists-forums-use-these.2681727.n2.nabble.com/</mailingListUrl>
    <bugTrackerUrl>https://github.com/supercollider/supercollider/issues</bugTrackerUrl>
    <releaseNotes>https://github.com/supercollider/sc-chocolatey/blob/master/supercollider/changelog.md</releaseNotes>
    <tags>supercollider admin audio</tags>
    <summary>SuperCollider install for Windows.</summary>
    <description>SuperCollider is a platform for audio synthesis and algorithmic composition, 
    used by musicians, artists, and researchers working with sound. It is free and open 
    source software available for Windows, macOS, and Linux.
    
    Requires the git.install package to be installed so that the SuperCollider Quarks feature
    works correctly.</description>
  <dependencies>
    <dependency id="git" version="2.33.1" />
  </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
